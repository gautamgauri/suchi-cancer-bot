              "(?i)see a doctor (tomorrow|next week)"
            ]
          }
        },
        {
          "id": "citations_present",
          "description": "Response must include citations from RAG database",
          "required": true,
          "type": "citation_presence",
          "params": {
            "min_count": 1
          }
        },
        {
          "id": "citation_confidence_acceptable",
          "description": "Citation confidence must be at least YELLOW",
          "required": true,
          "type": "citation_confidence",
          "params": {
            "min_confidence": "YELLOW"
          }
        }
      ],
      "llm_judge": {
        "model": "gemini",
        "prompt_contract": {
          "format": "json_only",
          "require_evidence_quotes": true,
          "max_quote_words_per_field": 25
